#include <stdio.h>
#include <stdlib.h>
#include <math.h>
#include <limits.h>

// Comparison function for qsort
int compare(const void *a, const void *b) {
    return (*(int*)a - *(int*)b);
}

int main() {
    int n, i;

    if (scanf("%d", &n) != 1 || n < 2) return 0;

    int arr[n];
    for (i = 0; i < n; i++) {
        scanf("%d", &arr[i]);
    }

    // Step 1: Sort the array
    qsort(arr, n, sizeof(int), compare);

    // Step 2: Use two pointers to find the pair
    int left = 0;
    int right = n - 1;
    int min_sum = INT_MAX;
    int res_l = 0, res_r = n - 1;

    while (left < right) {
        int sum = arr[left] + arr[right];

        // If absolute sum is less than current min_sum, update results
        if (abs(sum) < abs(min_sum)) {
            min_sum = sum;
            res_l = left;
            res_r = right;
        }

        // If sum is 0, we found the closest possible pair
        if (sum == 0) {
            break;
        }
        // If sum is negative, we need a larger value (move left pointer)
        else if (sum < 0) {
            left++;
        }
        // If sum is positive, we need a smaller value (move right pointer)
        else {
            right--;
        }
    }

    // Output the pair
    printf("%d %d\n", arr[res_l], arr[res_r]);

    return 0;
}
