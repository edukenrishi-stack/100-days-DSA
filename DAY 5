#include <stdio.h>

int main() {
    int p, q;
    
    // Read first log
    scanf("%d", &p);
    int log1[p];
    for (int i = 0; i < p; i++) {
        scanf("%d", &log1[i]);
    }
    
    // Read second log
    scanf("%d", &q);
    int log2[q];
    for (int i = 0; i < q; i++) {
        scanf("%d", &log2[i]);
    }
    
    // Merge the two sorted logs
    int merged[p + q];
    int i = 0, j = 0, k = 0;
    
    // Compare and merge while both logs have elements
    while (i < p && j < q) {
        if (log1[i] <= log2[j]) {
            merged[k++] = log1[i++];
        } else {
            merged[k++] = log2[j++];
        }
    }
    
    // Append remaining elements from log1 (if any)
    while (i < p) {
        merged[k++] = log1[i++];
    }
    
    // Append remaining elements from log2 (if any)
    while (j < q) {
        merged[k++] = log2[j++];
    }
    
    // Print the merged log
    for (int i = 0; i < p + q; i++) {
        printf("%d", merged[i]);
        if (i < p + q - 1) {
            printf(" ");
        }
    }
    printf("\n");
    
    return 0;
}
