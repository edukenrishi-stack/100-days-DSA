#include <stdio.h>

// Recursive function to compute n-th Fibonacci number
int fib(int n) {
    // Base cases
    if (n == 0) {
        return 0;
    }
    if (n == 1) {
        return 1;
    }
    
    // Recursive case: fib(n) = fib(n-1) + fib(n-2)
    return fib(n - 1) + fib(n - 2);
}

int main() {
    int n;
    
    // Read input
    scanf("%d", &n);
    
    // Compute and print result
    printf("%d\n", fib(n));
    
    return 0;
}
