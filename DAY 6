#include <stdio.h>

int main() {
    int n;
    
    // Read input
    scanf("%d", &n);
    int arr[n];
    for (int i = 0; i < n; i++) {
        scanf("%d", &arr[i]);
    }
    
    // Handle edge case
    if (n == 0) {
        return 0;
    }
    
    // Print first element (always unique)
    printf("%d", arr[0]);
    
    // Print subsequent unique elements
    for (int i = 1; i < n; i++) {
        if (arr[i] != arr[i - 1]) {
            printf(" %d", arr[i]);
        }
    }
    printf("\n");
    
    return 0;
}
